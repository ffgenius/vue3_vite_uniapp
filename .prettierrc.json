{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "useTabs": false,
  "bracketSpacing": true,
  "trailingComma": "none",
  "arrowParens": "always",
  "space-before-function-paren": true,
  "space-before-blocks": true,
  "space-in-parens": true
}
